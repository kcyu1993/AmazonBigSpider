{
  "UseShortFile": true,
  "Appenders": {
    "console": {
      "Type": "console"
    },
    "daylist": {
      "Type": "dailyfile",
      "Target": "/tmp/log/usa/daylist.log"
    },
    "dayasin": {
      "Type": "dailyfile",
      "Target": "/tmp/log/usa/dayasin.log"
    },
    "dayip": {
      "Type": "dailyfile",
      "Target": "/tmp/log/usa/dayip.log"
    }
  },
  "Loggers": {
    "daylist": {
      "Appenders": [
        "console",
        "daylist"
      ],
      "Level": "error"
    },
    "dayasin": {
      "Appenders": [
        "console",
        "dayasin"
      ],
      "Level": "error"
    },
    "dayip": {
      "Appenders": [
        "console",
        "dayip"
      ],
      "Level": "error"
    }
  },
  "Root": {
    "Level": "debug",
    "Appenders": [
      "console"
    ]
  }
}